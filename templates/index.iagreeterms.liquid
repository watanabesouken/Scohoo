{%- comment -%}Please DO NOT edit this file{%- endcomment -%}
        {%- layout none -%}
        {%- assign iatParamKey = 'q' -%}
        {%- assign iatParamValue = '' -%}
        {%- capture contentForQuerystring -%}{{ content_for_header }}{%- endcapture -%}
        {%- assign iatPageUrl = contentForQuerystring | split:'"pageurl":"' | last | split:'"' | first | split:'.myshopify.com' | last |
           replace:'\/','/' |
           replace:'%20',' ' |
           replace:'\u0026','&'
        -%}
        {%- assign pageQuerystring = iatPageUrl | split:'?' | last -%}
        {%- assign iatParts = pageQuerystring | split:'&' -%}
        {%- for iatPart in iatParts -%}
          {%- assign iatKeyAndValue = iatPart | split:'=' -%}
          {%- if iatKeyAndValue[0] == iatParamKey -%}
            {%- assign iatParamValue = iatKeyAndValue[1] | strip -%}
          {%- endif -%}
        {%- endfor -%}
        {%- assign namespace = 'td_' | append: iatParamValue -%}
        {
          "metafields": {
            "shop": {{ shop.permanent_domain | json }},
            "settings": {{ shop.metafields[namespace].app_settings }},
            "moneyFormat": {{ shop.money_format | json }},
            "namespace": "{{namespace}}"
          }
        }